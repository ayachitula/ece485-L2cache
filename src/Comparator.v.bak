//**************************************************
// Cache tag comparator:
// inputs: address tag, cache tag
// outputs: match
//**************************************************

module Comparator(
  addressTag,
  cacheTag,
  match
);
  input[4:0] addressTag;
  input[4:0] cacheTag;
  output reg match;

  always @(addressTag or cacheTag)
  begin
    assign match = addressTag && cacheTag;
  end
end module